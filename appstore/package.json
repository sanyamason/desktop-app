  "main": "dist/app.js",
  "scripts": {
    "build": "yarn run build:app && yarn run build:storybook",
    "build:app": "yarn run clean && yarn run gql-gen && webpack --config configs/webpack.config.js --env.build --env.sourceMap",
    "build:storybook": "yarn run gql-gen && cross-env NODE_ENV=storybook build-storybook -o dist/__storybook --quiet ",
    "clean": "rimraf dist storybook-static",
    "dev": "yarn run gql-gen && webpack-dev-server --config configs/webpack.config.js --open",
    "test": "jest",
    "reinstall": "rm -rf node_modules && yarn",
    "reinstall:win": "rd /s /q node_modules && yarn",
    "storybook": "yarn run gql-gen && cross-env NODE_ENV=storybook start-storybook -s ./src -p 6006",
    "gql-gen": "graphql-codegen --config codegen.yml",
    "gql-gen:watch": "graphql-codegen --config codegen.yml --watch",
    "tsc": "tsc -p . --noEmit",
    "tsc:watch": "tsc -p . --noEmit -w"
  },
  "jest": {
    "preset": "ts-jest"
  },
